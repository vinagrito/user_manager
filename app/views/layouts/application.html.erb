<!DOCTYPE html>
<html>
	<head>
	  <title>UserManager</title>
	  <%= stylesheet_link_tag "application", :media => "all" %>
	  <%= javascript_include_tag "application" %>
	  <%= csrf_meta_tags %>
	  <%=jqgrid_javascripts%>
	  <%=jqgrid_stylesheets%>
	</head>
	<body>
		<div>
			<%if flash[:notice]%>
				<p><%= flash[:notice]%></p>
			<%end%>
			<%if current_user%>
				Logged in as <%= current_user.username%> | <%= link_to 'logout', log_out_path%>
			<%end%>
		</div>

		<%= yield %>


	</body>
</html>
